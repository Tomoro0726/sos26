generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

generator markdown {
	provider = "prisma-markdown"
	output   = "./ERD.md"
	title    = "ERå›³"
}

model User {
  id    String @id @default(cuid())
  name  String
  email String @unique
}
